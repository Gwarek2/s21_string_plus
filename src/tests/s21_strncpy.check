#test test_s21_strncpy_not_empty_lines_n0
    char line1[20] = "hello\0";
    char line2[20] = "123\0";
    char* dest = line1;
    const char* src = line2;
    char line3[20] = "hello\0";
    char line4[20] = "123\0";
    char* dest2 = line3;
    const char* src2 = line4;
    s21_size_t n = 0;
    ck_assert_str_eq(s21_strncpy(dest, src, n), strncpy(dest2, src2, n));
