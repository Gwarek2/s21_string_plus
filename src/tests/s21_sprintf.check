    int c1 = s21_sprintf(buff1, "|%2.d|, |%010d|, |%+4i|", 0, -1234, 4219);
    int c2 = sprintf(buff2, "|%2.d|, |%010d|, |%+4i|", 0, -1234, 4219);
    ck_assert_str_eq(buff1, buff2);
